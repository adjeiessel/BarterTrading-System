<!DOCTYPE html>
<% include ../partials/header %>
<% include ../partials/navigation %>
<head>
    <title>Activity Log</title>
<script src="http://ajax.aspnetcdn.com/ajax/jQuery/jquery-1.11.1.min.js"></script>
<script  type="text/javascript" src="/javascripts/typeahead.min.js"></script>
<script>
    $(document).ready(function () {
        $('input.typeahead').typeahead({
            name: 'countries',
            remote: 'http://localhost:7000/productsearch?key=%QUERY',
            limit: 10
        });
    })
</script>
</head>
<div class="content">
    <div id ="ProductContent" name="ProductContent"class="content">
        <form class="contact_form" method="Get" name="contact_form" xmlns="http://www.w3.org/1999/html">
            <h1 style="color: #00b7ff">Activity Log</h1>
           <div class="add_mem">
                    <table id="example" width="100%" height="300em" >
                        <tr class="act_log">
                            <th>Activity</th>
                            <th>Date-Time</th>
                        </tr>
                        <%for (var i in data){%>
                        <tr>
                            <td> <%=data[i].ActivityName%></td>
                            <td><%=data[i].ActivityDateTime.toLocaleDateString()%>', '<%=data[i].ActivityDateTime.toLocaleTimeString()%></td>
                        </tr>
                        <%}%>
                    </table>
                </div>
        </form>
</div>
</div>
<% include ../partials/footer %>